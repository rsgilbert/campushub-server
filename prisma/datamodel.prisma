type User {
      id: ID! @id
      name: String!
      email: String! @unique
      password: String!
      parks: [Park!]!
      createdAt: DateTime! @createdAt
}


type Park {
    id: ID! @id
    createdAt: DateTime! @createdAt
    name: String
    location: Location
    managedBy: User
}


type Location {
    id: ID! @id
    place: String
    createdAt: DateTime! @createdAt
}
